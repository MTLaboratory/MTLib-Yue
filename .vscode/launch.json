{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch file",
            "type": "go",
            "request": "launch",
            "mode": "exec",
            "program": "${file}"
        },
        {
            "command": "moon.exe ${file}",
            "name": "Run npm start",
            "request": "launch",
            "type": "node-terminal"
        },
        {
            "name": "Run",
            "type": "lua",
            "request": "launch",
            "program": "${workspaceFolder}/${relativeFile}",
            "runtimeExecutable": "moon",
            "runtimeArgs": [
                "${workspaceFolder}/${relativeFile}"
            ],
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen"
        },
        {
            "name": "Compile",
            "type": "lua",
            "request": "launch",
            "program": "${workspaceFolder}/${relativeFile}",
            "runtimeExecutable": "moonc",
            "runtimeArgs": [],
            "env": {},
            "preLaunchTask": "Compile Moonscript to Lua",
            "postDebugTask": "Run Compiled Lua",
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen"
        }
    ]
}